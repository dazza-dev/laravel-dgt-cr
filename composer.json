{
  "name": "dazza-dev/laravel-dgt-cr",
  "description": "Paquete Laravel para generar, firmar y enviar documentos electrónicos (Factura, Nota crédito, Nota débito y Tiquete Electrónico) al Ministerio de Hacienda (Costa Rica).",
  "keywords": [
    "dgt",
    "dgt-laravel",
    "costa rica"
  ],
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=8.0",
    "dazza-dev/dgt-cr": "^1.0",
    "laravel/framework": "^8.0|^9.0|^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "larastan/larastan": "^2.0",
    "laravel/pint": "^1.18",
    "orchestra/testbench": "^8.27"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Andres Daza",
      "email": "adaza90@outlook.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "DazzaDev\\LaravelDgtCr\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DazzaDev\\LaravelDgtCr\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "DazzaDev\\LaravelDgtCr\\LaravelDgtCrServiceProvider"
      ],
      "aliases": {
        "LaravelDgtCr": "DazzaDev\\LaravelDgtCr\\Facades\\LaravelDgtCr"
      }
    }
  }
}
